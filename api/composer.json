{
  "name": "fxhos/api",
  "description": "fxhos项目后端api",
  "keywords": [
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.1.3",
    "doctrine/dbal": "^2.8",
    "guzzlehttp/guzzle": "^6.3",
    "hhxsv5/laravel-s": "~3.4.0",
    "illuminate/redis": "^5.7",
    "laravel/lumen-framework": "5.7.*",
    "league/flysystem": "^1.0",
    "maatwebsite/excel": "^3.1",
    "sentry/sentry-laravel": "^0.11.0",
    "spatie/laravel-permission": "^2.23",
    "tymon/jwt-auth": "1.0.*",
    "vlucas/phpdotenv": "~2.2",
    "ext-json": "*"
  },
  "require-dev": {
    "eaglewu/swoole-ide-helper": "dev-master",
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~7.0",
    "mockery/mockery": "~1.0"
  },
  "autoload": {
    "files": [
      "app/helpers.php"
    ],
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "release-script": [
      "@php artisan laravels publish --quiet",
      "@php artisan migrate --force",
      "@php artisan storage:link"
    ],
    "post-install-cmd": [
      "@release-script"
    ],
    "post-update-cmd": [
      "@release-script"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
